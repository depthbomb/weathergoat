model AlertDestination {
  id            Int     @id @default(autoincrement())
  uuid          String  @unique
  latitude      String
  longitude     String
  zoneId        String
  countyId      String
  guildId       String
  channelId     String
  autoCleanup   Boolean @default(true)
  radarImageUrl String?
  pingOnSevere  Boolean @default(false)
}
